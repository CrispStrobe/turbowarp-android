{
  "name": "turbowarp-android",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prebuild": "cd ../scratch-gui && npm run build",
    "build:web": "cd ../scratch-gui && npm run build",
    "sync": "npx cap sync",
    "sync:android": "npx cap sync android",
    "sync:ios": "npx cap sync ios",
    "build:android": "npm run build:web && node scripts/patch-index-for-capacitor.js && npx cap sync android",
    "build:ios": "npm run build:web && node scripts/patch-index-for-capacitor.js && npx cap sync ios",
    "build:all": "npm run build:web && node scripts/patch-index-for-capacitor.js && npx cap sync",
    "open:android": "npx cap open android",
    "open:ios": "npx cap open ios",
    "run:android": "npx cap run android",
    "run:ios": "npx cap run ios",
    "copy:extensions": "mkdir -p android/app/src/main/assets/public/extensions && mkdir -p android/app/src/main/assets/public/generated-metadata && cp ../extensions/extensions/*.js android/app/src/main/assets/public/extensions/ && cp ../extensions/extensions/extensions.json android/app/src/main/assets/public/generated-metadata/extensions-v0.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@capacitor/android": "^8.0.2",
    "@capacitor/app": "^8.0.1",
    "@capacitor/cli": "^8.0.2",
    "@capacitor/core": "^8.0.2",
    "@capacitor/ios": "^8.1.0",
    "cordova-plugin-bluetooth-serial": "^0.4.7"
  }
}
